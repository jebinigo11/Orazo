package com.example.loginformauth;

import org.springframework.security.crypto.bcrypt.BCryptPasswordEncoder;

public class TestHash {
    public static void main(String[] args) {
        BCryptPasswordEncoder encoder = new BCryptPasswordEncoder();
        System.out.println("CUST001: " + encoder.encode("password1"));
        System.out.println("CUST002: " + encoder.encode("password2"));
        System.out.println("CUST003: " + encoder.encode("password3"));
        // repeat for all 10 users
    }
}
